    <connection>DWH_LOG</connection>
    <sql>SELECT
 t.TRANSNAME as JOBNAME,
 &apos;transformation&apos; as JOBTYPE,
 t.ERRORS,
 t.LOG_FIELD
FROM
 ${KFF_LOGGING_TRNSF_TABLE} t
WHERE (STATUS=&apos;stop&apos; AND ERRORS &gt; 0) or
      (STATUS=&apos;start&apos; and not exists (select * 
                                        from  ${KFF_LOGGING_TRNSF_TABLE} s
                                       where s.STATUS in (&apos;end&apos;,&apos;stop&apos;) 
                                         and s.transname = t.transname
                                         and s.enddate = t.enddate))</sql>
