        <jsScript_script>&#47;&#47;Script here

var logConnection = getVariable(&quot;KFF_LOGGING_CONNECTION&quot;, &quot;???DWH_LOG???&quot;);
var logTable = getVariable(&quot;KFF_LOGGING_JOB_TABLE&quot;, &quot;???JOB_LOG???&quot;);

var log = Packages.org.pentaho.di.core.logging.LogWriter.getInstance();
var jobMeta = new Packages.org.pentaho.di.job.JobMeta(log, path, null);
var db = jobMeta.findDatabase(logConnection);

jobMeta.setLogConnection(db);
jobMeta.setLogTable(logTable);

var dos = new Packages.java.io.DataOutputStream(
   Packages.org.pentaho.di.core.vfs.KettleVFS.getOutputStream(path, false)
  );
dos.write(Packages.org.pentaho.di.core.xml.XMLHandler.getXMLHeader().getBytes(&quot;UTF-8&quot;));
dos.write(jobMeta.getXML().getBytes(&quot;UTF-8&quot;));
dos.close();
</jsScript_script>
