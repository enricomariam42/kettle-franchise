    <connection>LOGGING</connection>
    <sql>select 
 decode(t.transname,null,t.jobname,t.transname) jobname,
 t.stepname,
 t.error_descriptions,
 t.error_count,
 s.id,
 s.value,
 s.logdate
from 
  ${KFF_LOGGING_REJECT_TABLE} s,
 (select 
   jobname,
   transname,
   stepname,
   sum(error_count) error_count,
   error_descriptions,
   error_fields,
   error_codes,
   max(id) as id
 from
   ${KFF_LOGGING_REJECT_TABLE}
 group by 
   jobname,
   transname,
   stepname,
   error_descriptions,
   error_fields,
   error_codes ) t
where 
 t.id = s.id and
 decode(t.transname,null,t.jobname,t.transname) = decode(s.transname,null,s.jobname,s.transname) and
 t.stepname = s.stepname and
 t.error_descriptions = s.error_descriptions
</sql>
