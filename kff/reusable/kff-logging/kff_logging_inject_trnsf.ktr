        <jsScript_script>&#47;&#47;Script here

var logConnection = getVariable(&quot;KFF_LOGGING_CONNECTION&quot;, &quot;???DWH_LOG???&quot;);
var logTable = getVariable(&quot;KFF_LOGGING_TRNSF_TABLE&quot;, &quot;???TRANS_LOG???&quot;);

&#47;&#47; Alert(&quot;connection=&quot;+logConnection+&quot;, table=&quot;+logTable);

var version = Packages.org.pentaho.di.core.Const.VERSION;
var transMeta = new Packages.org.pentaho.di.trans.TransMeta(path);
var dos;

if (version.startsWith(&quot;3.&quot;)) {

  var db = transMeta.findDatabase(logConnection);
  transMeta.setLogConnection(db);
  transMeta.setLogTable(logTable);

} else if (version.startsWith(&quot;4.&quot;)) {

  var logTable = transMeta.getTransLogTable();
  logTable.setConnectionName(logConnection);
  logTable.setTableName(logTable);

}

dos = new Packages.java.io.DataOutputStream(
   Packages.org.pentaho.di.core.vfs.KettleVFS.getOutputStream(path, false)
  );
dos.write(Packages.org.pentaho.di.core.xml.XMLHandler.getXMLHeader().getBytes(&quot;UTF-8&quot;));
dos.write(transMeta.getXML().getBytes(&quot;UTF-8&quot;));
dos.close();
</jsScript_script>
